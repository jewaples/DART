														"sink": {
															"name": "UPC",
															"type": "Double"
														}
													},
													{
														"source": {
															"name": "product",
															"type": "String"
														},
														"sink": {
															"name": "PRODUCT",
															"type": "String"
														}
													},
													{
														"source": {
															"name": "brand",
															"type": "String"
														},
														"sink": {
															"name": "BRAND",
															"type": "String"
														}
													},
													{
														"source": {
															"name": "DATE",
															"type": "String"
														},
														"sink": {
															"name": "WEEK",
															"type": "DateTime"
														}
													},
													{
														"source": {
															"name": "delivery_volume",
															"type": "String"
														},
														"sink": {
															"name": "DELIVERIES",
															"type": "Int32"
														}
													},
													{
														"source": {
															"name": "dollar_volume",
															"type": "String"
														},
														"sink": {
															"name": "DOLLAR_VOLUME",
															"type": "Double"
														}
													},
													{
														"source": {
															"name": "unit_volume",
															"type": "String"
														},
														"sink": {
															"name": "UNIT_VOLUME",
															"type": "Double"
														}
													}
												]
											}
										},
										"inputs": [
											{
												"referenceName": "Ecommerce_Instacart_DataLake",
												"type": "DatasetReference",
												"parameters": {
													"path": "@pipeline().parameters.FolderPath_DestinationStore",
													"filename": "@item().name"
												}
											}
										],
										"outputs": [
											{
												"referenceName": "Ecommerce_Instacart_Sql",
												"type": "DatasetReference"
											}
										]
									},
									{
										"name": "Copy_Archive",
										"type": "Copy",
										"dependsOn": [
											{
												"activity": "Validate_Destination_Data",
												"dependencyConditions": [
													"Succeeded"
												]
											}
										],
										"policy": {
											"timeout": "7.00:00:00",
											"retry": 0,
											"retryIntervalInSeconds": 30,
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"source": {
												"type": "DelimitedTextSource",
												"storeSettings": {
													"type": "AzureBlobFSReadSettings",
													"recursive": false,
													"enablePartitionDiscovery": false
												},
												"formatSettings": {
