										"name": "Get_AdlMetadata_Archive",
										"type": "GetMetadata",
										"dependsOn": [
											{
												"activity": "Copy_Archive",
												"dependencyConditions": [
													"Succeeded"
												]
											}
										],
										"policy": {
											"timeout": "7.00:00:00",
											"retry": 0,
											"retryIntervalInSeconds": 30,
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"dataset": {
												"referenceName": "RAD_File_Azure_Storage_Destination",
												"type": "DatasetReference",
												"parameters": {
													"path": {
														"value": "@concat(pipeline().parameters.FolderPath_DestinationStore,'/' ,'Archive' )",
